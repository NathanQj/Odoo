        <record id="view_move_line_ext_form" model="ir.ui.view">
            <field name="name">view.move.line.ext.form</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock_barcode.stock_move_line_product_selector"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='qty_done']" position="after">
                    <field name="price_subtotal" readonly="0"/>
                    <field name="standard_price"/>
                    <field name="list_price" readonly="0"/>
                    <field name="product_barcode" readonly="1"/>
                    <field name="profit_margin"/>
                </xpath>


                <xpath expr="//field[@name='owner_id']" position="after">
                    <field name="description" readonly="0"/>
                </xpath>


            </field>
        </record>


    </data>
